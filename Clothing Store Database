SELECT * FROM inventory;
SELECT * FROM inventory WHERE price >60;
SELECT * FROM inventory WHERE color ORDER BY price;

SELECT SUM(price) FROM inventory;
SELECT SUM(color) FROM inventory GROUP BY price;
